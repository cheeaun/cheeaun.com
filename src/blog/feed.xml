<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title type="text">cheeaunblog</title>
	<id>tag:cheeaun.com,2011:blog</id>
	<link rel="alternate" type="text/html" href="{{domainURL}}/blog"/>
	<link rel="self" type="application/atom+xml" href="{{domainURL}}/blog/feed.xml"/>
	<updated>{{latestPosts.0.iso8601Date}}</updated>
	<author>
		<name>{{authorName}}</name>
	</author>
	{{#each latestPosts}}
		<entry>
			<id>{{../domainURL}}{{permalink}}</id>
			<updated>{{iso8601Date}}</updated>
			<title type="text">{{title}}</title>
			<summary type="text">{{{summary}}}</summary>
			<link rel="alternate" type="text/html" href="{{url}}" title="{{title}}"/>
		</entry>
	{{/each}}
</feed>
